{"code":"import React, { useState } from \"react\";\r\nimport DefaultDrawer from \"./DefaultDrawer\";\r\nimport \"./style.scss\";\r\n/*\r\n    undefined : typeof instance === \"undefined\"\r\n    Boolean : typeof instance === \"boolean\"\r\n    Number : typeof instance === \"number\"\r\n    String : typeof instance === \"string\"\r\n    BigInt : typeof instance === \"bigint\"\r\n    Symbol : typeof instance === \"symbol\"\r\n    */\r\n// create usual filters like\r\n// shadow (don't draw the element himself but draw children)\r\n// don't draw\r\n// REFLECHIRE A UNE ARBORESCENCE DE DRAWER (CASCADING)\r\nfunction Print(props) {\r\n    // init here\r\n    // => LOOP DETECTOR (circulary refence, mayby use symbol ?)\r\n    // give absolute_path for filters ?\r\n    // recursive drawer will be more efficient and beautiful to write\r\n    var name = props.name;\r\n    var value = props.value;\r\n    var drawer = props.drawer ? props.drawer : DefaultDrawer;\r\n    var deepness = 0;\r\n    var maxDeepness = 40;\r\n    return (React.createElement(\"div\", { className: \"printer\" }, React.createElement(Loop, { name: name, value: value, drawer: drawer, deepness: deepness, maxDeepness: maxDeepness })));\r\n}\r\nfunction Loop(props) {\r\n    var _a = useState(props.deepness < props.maxDeepness), isOpen = _a[0], setIsOpen = _a[1];\r\n    var child = null;\r\n    // transformer Loop en FONCTION NON REACT\r\n    // could return info from child\r\n    // could return total nblines\r\n    var l;\r\n    var maxDeepness = isOpen ? props.maxDeepness : 0;\r\n    if (props.value.constructor === Array) {\r\n        l = props.value.length;\r\n        if (!isOpen && l >= 3)\r\n            l = 3;\r\n        child = new Array(l);\r\n        for (var i = 0; i < l; i++) {\r\n            child[i] = (React.createElement(Loop, { name: i, value: props.value[i], drawer: props.drawer, deepness: props.deepness + 1, maxDeepness: maxDeepness }));\r\n        }\r\n    }\r\n    else if (props.value.constructor === Object) {\r\n        var entries = Object.entries(props.value);\r\n        l = entries.length;\r\n        if (!isOpen && l >= 3)\r\n            l = 3;\r\n        child = new Array(l);\r\n        for (var i = 0; i < l; i++) {\r\n            child[i] = (React.createElement(Loop, { name: entries[i][0], value: entries[i][1], drawer: props.drawer, deepness: props.deepness + 1, maxDeepness: maxDeepness }));\r\n        }\r\n    }\r\n    for (var i = 0, l_1 = props.drawer.length; i < l_1; i++) {\r\n        var e = props.drawer[i];\r\n        if (e.filter(props)) {\r\n            var Component = e.Component;\r\n            return (React.createElement(Component, { name: props.name, value: props.value, isOpen: isOpen, setIsOpen: setIsOpen }, child));\r\n        }\r\n    }\r\n}\r\nexport default Print;\r\n//# sourceMappingURL=Printer.js.map","references":["/home/c141/npmC141/node_modules/@types/react/index.d.ts","/home/c141/npmC141/src/Printer/DefaultDrawer.tsx"],"map":"{\"version\":3,\"file\":\"Printer.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/Printer/Printer.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAE,QAAQ,EAAE,MAAM,OAAO,CAAC;AACxC,OAAO,aAAa,MAAM,iBAAiB,CAAC;AAC5C,OAAO,cAAc,CAAC;AAEtB;;;;;;;MAOG;AAEH,4BAA4B;AAC5B,4DAA4D;AAC5D,aAAa;AAEb,sDAAsD;AAEtD,SAAS,KAAK,CAAC,KAAU;IACvB,YAAY;IACZ,2DAA2D;IAC3D,mCAAmC;IACnC,iEAAiE;IAEjE,IAAI,IAAI,GAAG,KAAK,CAAC,IAAI,CAAC;IACtB,IAAI,KAAK,GAAG,KAAK,CAAC,KAAK,CAAC;IACxB,IAAI,MAAM,GAAG,KAAK,CAAC,MAAM,CAAC,CAAC,CAAC,KAAK,CAAC,MAAM,CAAC,CAAC,CAAC,aAAa,CAAC;IACzD,IAAI,QAAQ,GAAG,CAAC,CAAC;IACjB,IAAI,WAAW,GAAG,EAAE,CAAC;IAErB,OAAO,CACL,6BAAK,SAAS,EAAC,SAAS,IAEpB,oBAAC,IAAI,IACH,IAAI,EAAE,IAAI,EACV,KAAK,EAAE,KAAK,EACZ,MAAM,EAAE,MAAM,EACd,QAAQ,EAAE,QAAQ,EAClB,WAAW,EAAE,WAAW,GACxB,CAEA,CACP,CAAC;AACJ,CAAC;AAED,SAAS,IAAI,CAAC,KAAU;IAChB,IAAA,KAAsB,QAAQ,CAAC,KAAK,CAAC,QAAQ,GAAG,KAAK,CAAC,WAAW,CAAC,EAAjE,MAAM,QAAA,EAAE,SAAS,QAAgD,CAAC;IACzE,IAAI,KAAK,GAAG,IAAI,CAAC;IAEjB,yCAAyC;IACzC,+BAA+B;IAC/B,6BAA6B;IAE7B,IAAI,